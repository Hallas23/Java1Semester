package application.model;

import java.time.LocalDate;
import java.time.LocalDateTime;

public class Test {

	public static void main(String[] args) {
		
		Deltager et = new Deltager("Ole Jensen", 24, "Jensen vej", "15-11-2018, "Århus", "eaa",  88887777, 22224444);
		Deltager to = new Deltager("Ole Wedel", 28, "egon vej", "Århus", "mongoTV", 88887777, 22224444);
		
		Hotel hotel1 = new Hotel("Svanen", "jern 25", 3, 200, 300, 400);		
		HotelTillæg tillæg1 = new HotelTillæg(false, false, true);
		Hotel hotel2 = new Hotel("Ørnen", "jern tosser", 3, 600, 2000, 400);		
		HotelTillæg tillæg2 = new HotelTillæg(true, true, true);
		
		
		Udflugt Herning = new Udflugt("Tur til Herning museum", "Herning", LocalDateTime.of(2018, 11, 15, 12, 30), LocalDateTime.of(2018, 11, 15, 16, 30), 300);

		Tilmelding en = new Tilmelding("1", "MarkHansen", true, et, Herning, hotel1, tillæg1);
		Tilmelding ee = new Tilmelding("1", "MarkHansen", true, to, Herning, hotel2, tillæg2);
		
		System.out.println(en);
		System.out.println();
		System.out.println(ee);
		
	}

}
