package Opgave5;

import java.util.ArrayList;

/**
 * Models a training plan for a Swimmer
 */
public class Episode {
	private int number;
	private String guest;
	private int length;

	Episode(int number, String guest, int length) {
		this.number = number;
		this.guest = guest;
		this.length = length;
	}

	public int getLength() {
		return length;
	}

	public void setLength(int length) {
		this.length = length;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}
	
}
