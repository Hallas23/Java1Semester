package lektion8;

public class E602 {

	public static void main(String[] args) {
		
		int x = 0;
		int to = 0;
		
			while(to <= 100) {
				to = to + 2;
				x = x + to;
		
		}
		System.out.println(x);
	
		
	int y = 0;
	int i = 1;
	
		while(i <= 100) {
			if(Math.sqrt(i) - (int)Math.sqrt(i)==0)
			y = y + i;
			i++;
	
	}
	System.out.println(y);
	

	double u = 0;
	int t = 0;
	
		while(t <= 20) {
			u = u + Math.pow(2, t++);
	
		}
	System.out.println(u);
	}
}
